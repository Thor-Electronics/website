{
  "name": "iot-website",
  "private": true,
  "sideEffects": false,
  "//type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "//dev": "run-p dev:*",
    "//dev:remix": "remix dev",
    "//dev:css": "postcss styles --base styles --dir app/styles -w",
    "//build": "run-s build:*",
    "//build:css": "postcss styles --base styles --dir app/styles --env production",
    "//start": "remix-serve build",
    "//build:remix": "cross-env NODE_ENV=production remix build",
    "//build-worker": "esbuild ./app/entry.workbox.ts --outfile=./public/entry.workbox.js --minify --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "//dev:worker": "esbuild ./app/entry.workbox.ts --outfile=./public/entry.workbox.js --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.1.1",
    "@mui/material": "^5.15.10",
    "@mui/x-data-grid": "^6.19.5",
    "@prisma/client": "^5.10.2",
    "@remix-run/css-bundle": "2.7.2",
    "@remix-run/node": "2.7.2",
    "@remix-run/react": "2.7.2",
    "@remix-run/serve": "2.7.2",
    "@sentry/remix": "^7.102.1",
    "axios": "^1.6.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "google-map-react": "^2.2.1",
    "isbot": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-use-websocket": "^4.7.0",
    "tiny-invariant": "^1.3.3",
    "web-push": "^3.6.7",
    "workbox-background-sync": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "^5.1.1",
    "@remix-run/dev": "2.7.2",
    "@remix-run/eslint-config": "2.7.2",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "postcss-nested": "^6.0.1",
    "postcss-scss": "^4.0.9",
    "prisma": "^5.10.2",
    "sass": "^1.71.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
