{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "build": "run-s build:*",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "start": "remix-serve build",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "_build-worker": "esbuild ./app/entry.workbox.ts --outfile=./public/entry.workbox.js --minify --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "_dev:worker": "esbuild ./app/entry.workbox.ts --outfile=./public/entry.workbox.js --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.1.1",
    "@mui/material": "^5.15.7",
    "@mui/x-data-grid": "^6.19.3",
    "@prisma/client": "^5.9.1",
    "@remix-run/node": "2.6.0",
    "@remix-run/react": "2.6.0",
    "@remix-run/serve": "2.6.0",
    "@sentry/remix": "^7.99.0",
    "axios": "^1.6.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "google-map-react": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-use-websocket": "^4.5.0",
    "tiny-invariant": "^1.3.1",
    "web-push": "^3.6.7",
    "workbox-background-sync": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "^5.1.1",
    "@remix-run/dev": "2.6.0",
    "@remix-run/eslint-config": "2.6.0",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-nested": "^6.0.1",
    "postcss-scss": "^4.0.9",
    "prisma": "^5.9.1",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16"
  }
}
