{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "build": "run-s build:*",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "start": "remix-serve build",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build-worker": "esbuild ./app/entry.workbox.ts --outfile=./public/entry.workbox.js --minify --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "build-render": "NODE_ENV=production remix build",
    "dev:worker": "esbuild ./app/entry.workbox.ts --outfile=./public/entry.workbox.js --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"development\"' --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.0.18",
    "@mui/material": "^5.14.6",
    "@mui/x-data-grid": "^6.11.2",
    "@prisma/client": "^5.2.0",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@sentry/remix": "^7.64.0",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "google-map-react": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-use-websocket": "^4.3.1",
    "tiny-invariant": "^1.3.1",
    "web-push": "^3.6.4",
    "workbox-background-sync": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "@remix-pwa/sw": "*",
    "@remix-pwa/push": "*"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "^5.0.1",
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "postcss-cli": "^10.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-scss": "^4.0.7",
    "prisma": "^5.2.0",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16"
  }
}
