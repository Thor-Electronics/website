{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "build": "run-s build:*",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "start": "remix-serve build",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:worker": "esbuild ./app/entry.workbox.ts --outfile=./public/entry.workbox.js --minify --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:worker": "esbuild ./app/entry.workbox.ts --outfile=./public/entry.workbox.js --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"development\"' --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@heroicons/react": "^2.0.11",
    "@mui/material": "^5.11.3",
    "@mui/x-data-grid": "^5.17.18",
    "@prisma/client": "^4.3.1",
    "@remix-run/node": "^1.6.8",
    "@remix-run/react": "^1.6.8",
    "@remix-run/serve": "^1.6.8",
    "@sentry/remix": "^7.51.0",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "google-map-react": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-use-websocket": "^4.2.0",
    "tiny-invariant": "^1.2.0",
    "web-push": "^3.6.1",
    "workbox-background-sync": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "^5.0.1",
    "@remix-run/dev": "^1.6.8",
    "@remix-run/eslint-config": "^1.6.8",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.20.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-nested": "^5.0.6",
    "postcss-scss": "^4.0.4",
    "prisma": "^4.3.1",
    "sass": "^1.54.5",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16"
  }
}
